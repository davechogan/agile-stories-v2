{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "secretsmanager:GetSecretValue",
                "secretsmanager:DescribeSecret",
                "secretsmanager:GetResourcePolicy",
                "dynamodb:*",
                "lambda:*",
                "iam:*",
                "s3:*",
                "ec2:*",
                "apigateway:*",
                "logs:*",
                "cloudwatch:*",
                "sqs:*"
            ],
            "Resource": "*"
        }
    ]
}
